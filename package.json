    "dist",
    "public",
    "src",
    "index.ts",
    "preset.js"
  ],
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css,vue}": "npm run lint",
    "*.{js,jsx,ts,tsx,md,html,css,vue}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "consola": "3.0.1"
    }
  },
  "overrides": {
    "consola": "3.0.1"
  },
  "resolutions": {
    "consola": "^3.0.0"
  }
