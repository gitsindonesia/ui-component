{
  "name": "@morpheme/app-bar",
  "version": "1.0.1",
  "description": "Morpheme AppBar is a Vue component used to display content at the top of a page.",
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "keywords": [
    "app-bar",
    "nav-bar",
    "tailwind-config",
    "morpheme"
  ],
  "author": "Warsono <warsono16694@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@morpheme/theme": "^1.0.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@morpheme/button": "^1.0.1",
    "@morpheme/icon": "^1.0.1",
    "@morpheme/tailwind-config": "^1.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "c8": "^7.11.3",
    "tailwindcss": "^3.3.1",
    "vite": "^4.3.8",
    "vitest": "^0.12.4"
  },
  "main": "dist/app-bar.umd.js",
  "unpkg": "dist/app-bar.iife.js",
  "jsdelivr": "dist/app-bar.iife.js",
  "module": "./dist/app-bar.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/app-bar.mjs",
      "require": "./dist/app-bar.js"
    },
    "./package.json": "./package.json",
    "./src/*": "./src/*",
    "./*": "./*"
  },
  "gitHead": "92215d91d1e3a2b1b804e52b55dcfd0ed5274475"
