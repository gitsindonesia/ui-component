{
  "name": "@morpheme/text",
  "version": "1.0.0",
  "description": "Morpheme Text Component",
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "keywords": [
    "text",
    "morpheme",
    "ui-component"
  ],
  "author": "Warsono <warsono16694@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@morpheme/utils": "^1.0.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@morpheme/tailwind-config": "^1.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "c8": "^7.11.3",
    "tailwindcss": "^3.3.1",
    "vite": "^4.3.8",
    "vitest": "^0.12.4"
  },
  "main": "dist/text.umd.js",
  "unpkg": "dist/text.iife.js",
  "jsdelivr": "dist/text.iife.js",
  "module": "./dist/text.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/text.mjs",
      "require": "./dist/text.js"
    },
    "./package.json": "./package.json",
    "./src/*": "./src/*",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  }
}
