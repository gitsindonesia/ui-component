    "@gits-id/icon": "^1.0.0-beta.2",
    "@gits-id/theme": "^1.0.0-beta.2",
    "@gits-id/utils": "^1.0.0-beta.2",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.3",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite": "^3.0.0",
