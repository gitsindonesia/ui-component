  "license": "MIT",
  "dependencies": {
    "@heroicons/vue": "^1.0.6",
    "@iconify/vue": "^3.2.1",
    "@morpheme/theme": "^1.0.0-rc.0",
    "@morphemeicons/vue": "^0.2.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@morpheme/tailwind-config": "^1.0.0-rc.0",
    "vite": "^4.3.8",
